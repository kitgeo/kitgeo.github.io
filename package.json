{
  "name": "kitgeo",
  "version": "0.2.0",
  "description": "Kitgeo site",
  "main": "index.html",
  "scripts": {
    "start": "jekyll serve",
    "build": "npm run copy:img && npm run build:assets && npm run build:site",
    "build:assets": "webpack",
    "build:site": "jekyll build",
    "copy:img": "copyfiles -u 2 -s \"src/img/**/*\" assets/img",
    "watch:assets": "webpack --watch",
    "clean": "npm run clean:assets && npm run clean:site",
    "clean:assets": "rimraf assets",
    "clean:site": "rimraf _site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kitgeo/kitgeo.github.io"
  },
  "author": "Dmitriy Pushkov",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/kitgeo/kitgeo.github.io/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "~2.1.1",
    "jquery-form-validator": "^2.3.64",
    "jquery.stellar": "^0.6.2",
    "lightbox2": "^2.9.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1"
  }
}
